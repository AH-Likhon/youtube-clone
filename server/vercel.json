{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "dest": "./routers/auth.js"
    },
    {
      "src": "/api/videos",
      "dest": "./routers/videos.js"
    },
    {
      "src": "/api/comments",
      "dest": "./routers/comments.js"
    },
    {
      "src": "/api/users",
      "dest": "./routers/users.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
